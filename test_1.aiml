<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">
	<category>
        <pattern> OI </pattern>
        <template>Ola como voce se chama?</template>
    </category>
    <category>
        <pattern>ME CHAMO *</pattern>
        <template>Fala aí <set name="name"><star/></set>, como posso te ajudar?</template>
    </category>
    <category>
        <pattern>COMO E MEU NOME</pattern>
        <template>
            <random>
            <li><get name="name"/>!</li>
            <li>Seu nome é <get name="name"/></li>
            <li>Tá esquecido hein <get name="name"/>?</li>
            </random>
        </template>
    </category>
    <category>
        <pattern>*</pattern>
        <template>Não entendi, pode falar de outra forma?</template>
    </category>
</aiml>